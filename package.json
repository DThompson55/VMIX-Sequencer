{
  "name": "vmix-sequencer",
  "productName": "vmix-sequencer",
  "version": "0.1.0",
  "description": "A front end for vMix",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "node test/tester.js"
  },
  "keywords": [],
  "author": "danielthompson",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "vmix-sequencer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
